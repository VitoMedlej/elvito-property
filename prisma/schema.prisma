generator client {
  provider = "prisma-client-js"
  binaryTargets = ["native", "debian-openssl-1.1.x"]

}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

type FeaturedOwnerDetails {
  ownerEmail        String
  ownerId           String
  ownerName         String
  ownerPhoneNumber  String
  ownerProfileImage String
}

type PropertiesOwnerDetails {
  ownerEmail        String
  ownerId           String
  ownerName         String
  ownerPhoneNumber  String
  ownerProfileImage String
}

model Featured {
  id            String               @id @default(auto()) @map("_id") @db.ObjectId
  balconies     Int
  bathrooms     Int
  createdAt     String
  currency      String
  description   String
  images        String[]
  isFurnished   Boolean
  keywords      String[]
  location      String
  ownerDetails  FeaturedOwnerDetails
  paymentMethod String
  price         Int
  propertySize  String
  purpose       String
  rentFrequency String
  rooms         Int
  slug          String
  state         String
  title         String
  type          String
}

model Properties {
  id            String                 @id @default(auto()) @map("_id") @db.ObjectId
  balconies     Int
  bathrooms     Int
  createdAt     String
  currency      String
  description   String
  images        String[]
  isFurnished   Boolean
  keywords      String[]
  location      String
  ownerDetails  PropertiesOwnerDetails
  paymentMethod String
  price         Int
  propertySize  String
  purpose       String
  rentFrequency String
  rooms         Int
  slug          String
  state         String
  title         String
  type          String
}

model Users {
  id           String @id @default(auto()) @map("_id") @db.ObjectId
  userEmail    String
  userImage    String
  userName     String
  userPassword String
  userPhone    String
}
